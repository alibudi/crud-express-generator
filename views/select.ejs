<h1>Tabel data</h1>
<a href="/form">Insert</a>
<table>
    <tr>
        <td>id</td>
        <td>name</td>
        <td>price</td>
    </tr>
    <% books.forEach(function(item) {%>
    <tr>
        <td><%= item.id %></td>
        <td><%= item.name %></td>
        <td><%= item.price %></td>
        <td>
            <a href="/delete?id=<%= item.id %>" onclick="return confirm('are you sure ?')">Delete</a>
            <a href="/edit?id=<%= item.id %>">Edit</a>
        </td>
    </tr>
   <% }); %>
</table>